{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"eofline": false,
		"indent": [true, "tabs", 4],
		"max-classes-per-file": [false],
		"max-line-length": [true, 180],
		"trailing-comma": [true, { "multiline": "ignore", "singleline": "ignore" }],
		"variable-name": [true, "check-format", "allow-leading-underscore"]
	},
	"rulesDirectory": []
}